# samo_a1.map
# FLASH = PM25LV512
# SIZE  = 64kB

0x1	mbr
0x0300  menu

0x2000  rs232-loader.header
0x2300  rs232-loader

0x4000  forth-loader.header
0x4300  forth-loader

0x6000  lcd-test.header
0x6300	lcd-test

0x8000	key-test.header
0x8300	key-test

0xa000	memory-test.header
0xa300	memory-test

0xc000  hello.header
0xc300  hello

0xe000  kernel-loader.header
0xe300  kernel-loader
